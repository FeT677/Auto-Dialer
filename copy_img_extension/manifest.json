{
  "manifest_version": 3,
  "name": "Авито",
  "version": "3000",
  "description": "Копирует номер телефона в формате PNG",
  "permissions": [
    "clipboardWrite"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"]
    }
  ]
}
